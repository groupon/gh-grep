{
  "name": "gh-grep",
  "version": "0.0.0",
  "description": "GitHub CLI grep extension",
  "private": true,
  "license": "BSD-3-Clause",
  "main": "lib/gh-grep.js",
  "bin": {
    "gh-grep": "./cli.js"
  },
  "homepage": "https://github.com/groupon/gh-grep",
  "repository": {
    "type": "git",
    "url": "https://github.com/groupon/gh-grep"
  },
  "bugs": {
    "url": "https://github.com/groupon/gh-grep/issues"
  },
  "scripts": {
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint .",
    "pretest": "npm-run-all pretest:*",
    "pretest:ts": "npm run ts",
    "pretest:lint": "npm-run-all lint:*",
    "test": "npm-run-all test:*",
    "test:unit": "c8 mocha",
    "posttest": "npm-run-all posttest:*",
    "posttest:nlm": "nlm verify",
    "ts": "rm -rf lib && tsc",
    "watch": "tsc --watch"
  },
  "c8": {
    "reporter": [
      "text",
      "json-summary"
    ],
    "exclude": [
      "example{,s,-app}/**",
      "**/*.d.ts",
      "**/*{.,-,c}{spec,test}.{js,cjs,mjs,ts}",
      "{spec,test}{,s}/**"
    ]
  },
  "engines": {
    "node": ">=14"
  },
  "mocha": {
    "recursive": true,
    "extension": [
      "test.js",
      "test.cjs",
      "test.mjs",
      "test.ts"
    ],
    "require": "ts-node/register"
  },
  "nlm": {
    "license": {
      "files": [
        "src"
      ]
    }
  },
  "dependencies": {
    "@octokit/plugin-request-log": "^1.0.4",
    "@octokit/rest": "^19.0.4",
    "chalk": "^2.4.2",
    "commander": "^9.4.0",
    "debug": "^3.1.0",
    "promise-limit": "^2.7.0"
  },
  "devDependencies": {
    "@octokit/openapi-types": "^13.3.0",
    "@octokit/request-error": "^3.0.1",
    "@tsconfig/node14": "^1.0.0",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^9.0.1",
    "@types/node": "^14",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "c8": "^7.7.3",
    "declarative-nock": "^1.0.1",
    "eslint": "^7.29.0",
    "eslint-config-groupon": "^11.0.4",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.4.1",
    "mocha": "^9.0.1",
    "nlm": "^5.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  },
  "author": {
    "name": "Groupon, Inc.",
    "email": "opensource@groupon.com"
  },
  "files": [
    "*.js",
    "lib",
    "*.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
